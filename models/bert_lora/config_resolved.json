{
  "run": "bert_lora",
  "base_model": "bert-base-cased",
  "use_lora": true,
  "dataset": "mrm8488/stackoverflow-ner",
  "seed": 13,
  "max_length": 128,
  "label_all_tokens": false,
  "num_labels": 11,
  "label_names": [
    "O",
    "B-ERROR",
    "B-HARDWARE",
    "B-OS",
    "B-SOFTWARE",
    "B-VERSION",
    "I-ERROR",
    "I-HARDWARE",
    "I-OS",
    "I-SOFTWARE",
    "I-VERSION"
  ],
  "fp16": true,
  "tf32": true,
  "gradient_checkpointing": true,
  "max_train_samples": null,
  "max_eval_samples": null,
  "effective_batch": 16,
  "lr_effective": 0.0001,
  "label_smoothing": 0.05,
  "lr_scheduler": "cosine",
  "max_grad_norm": 1.0,
  "metric_for_best_model": "eval_f1",
  "greater_is_better": true,
  "early_stopping_patience": 1,
  "early_stopping_threshold": 0.0,
  "group_by_length": true,
  "lora": {
    "enabled": true,
    "r": 16,
    "alpha": 32,
    "dropout": 0.05,
    "bias": "none",
    "target_modules": [
      "query",
      "key",
      "value",
      "dense"
    ],
    "modules_to_save": [
      "classifier"
    ],
    "lr": 0.0001
  },
  "model_cfg": {
    "hidden_dropout_prob": 0.1,
    "attention_probs_dropout_prob": 0.1,
    "classifier_dropout": 0.2,
    "dropout": 0.1,
    "attention_dropout": 0.1
  },
  "batch_size": 16,
  "grad_accum": 1,
  "epochs": 6.0,
  "lr": 0.0001,
  "weight_decay": 0.01,
  "warmup_ratio": 0.06,
  "eval_batch_size": 16,
  "dataloader_num_workers": 4
}